<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="..\css\busquedasUsuario.css">
    <link rel="icon" type="img/png" href="">
    <title>Busqueda Usuario</title>
</head>
<body>

    <%-include("paginaPrincipal")%>
    <div class="titulo2">
        <h2>Usuarios</h2>
    </div>

    <div class="filtrador">
        <input class= "inFil" type="search" name="buscador" id="">
        <button class="filtrar" type="submit">Filtrar</button>
    </div>

    <div class="contenedorUsuarios">
        
        <%usersList.forEach(function(user){%>
            <div class="usuario">
                <div class="imagen">
                    <img src="/imagenUsuario/<%=user.imagen%>">
                </div>
                <div class="nombre">
                    <a class="estiloEnlace"><%=user.nombre%></a>
                </div>
                <div class="reputacion">
                    <h3><%=user.reputacion%></h3>
                </div>
                <div class="tags">
                    <a class="tag">Tais</a>
                </div>
            </div>
        <%})%>
    </div>

</body>
</html>